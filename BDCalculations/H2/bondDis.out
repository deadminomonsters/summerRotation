e_fit = e5[start:end]
p0 = [min(e_fit), x[np.argmin(e_fit)], 0.5]
params, cov = curve_fit(harmonic, x_fit, e_fit, p0=p0)
V0, Req, k = params
# Compute vibrational frequency
nu_cm1 = (1 / (2 * np.pi * c)) * np.sqrt(k * conv)
print('For CASSCF')
