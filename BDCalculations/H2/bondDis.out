        basis = 'ccpvdz',
        charge = 0,
        spin = 0,
        symmetry = True
    )

    #RHF
    rh = scf.RHF(mol)
    rh_energy = rh.kernel()

    #UHF
    uh = scf.UHF(mol)
    uh_energy = uh.kernel()

    #RCCSD
    rcc = cc.CCSD(rh)
    rcc.kernel()
    rcc_energy = rcc.e_tot


    #UCCSD 
    ucc = cc.UCCSD(uh)
    ucc.kernel()
    ucc_energy = ucc.e_tot

    #CASSCF (4e,4o)
    ca = mcscf.CASSCF(rh, ncas=2, nelecas=2)
    ca_energy =ca.kernel()[0]

    return rh_energy, uh_energy, rcc_energy, ucc_energy, ca_energy

